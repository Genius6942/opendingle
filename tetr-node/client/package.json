    "node-fetch": "^2.6.9",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.12",
    "@types/node-fetch": "^2.6.3",
    "@types/ws": "^8.5.4",
    "tsup": "^6.7.0",
    "typedoc": "^0.24.6",
    "typedoc-plugin-missing-exports": "^2.0.0",
    "typescript": "^5.0.4"
  }
